 n = 10 
 a, b = 0, 1
 print("Fibonacci Series:")
 for i in range(n):
    print(a, end=" ")
    a, b = b, a + b

rows = 5
 for i in range(1, rows+1):
    for j in range(1, i+1):
        print(j, end=" ")
    print()

rows = 5
 k = 0
 for i in range(1, rows+1):
    for space in range(1, (rows-i)+1):
print(end=" ")
while k != (2*i-1)
    print("*",end = " ")
  k += 1
K = 0
print()
